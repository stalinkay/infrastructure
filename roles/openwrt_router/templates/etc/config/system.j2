{{ ansible_managed | comment }}

config system
	option hostname '{{ ansible_hostname }}'
	option zonename 'America/Boise'
	option timezone 'MST7MDT,M3.2.0,M11.1.0'
	option conloglevel '8'
	option cronloglevel '8'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled '1'

config led 'led_wan'
	option name 'WAN'
	option sysfs 'pca963x:shelby:white:wan'
	option trigger 'netdev'
	option dev 'eth0'
	option mode 'link tx rx'

config led 'led_usb2'
	option name 'USB2'
	option sysfs 'pca963x:shelby:white:usb2'
	option trigger 'usbdev'
	option dev '1-1'
	option interval '50'

config led 'led_usb3'
	option name 'USB3'
	option sysfs 'pca963x:shelby:white:usb3_1'
	option trigger 'usbdev'
	option dev '2-1'
	option interval '50'
